{
    "version": "2.0",
    "extensions": {
        "http": {
            "maxConcurrentRequests": 1,
            "maxOutstandingRequests": 5
        }
    },
    "logging": {
        "applicationInsights": {
            "samplingSettings": {
                "isEnabled": true,
                "excludedTypes": "Request"
            },
            "enableLiveMetricsFilters": true
        }
    }
}